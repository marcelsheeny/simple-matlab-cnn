classdef input_layer < handle
    %INPUT_DATA Summary of this class goes here
    %   Detailed explanation goes here
    
    properties
        size;
        out_size;
    end
    
    methods
        function obj = input_layer(siz)
           obj.size = siz;
           obj.out_size = size(3);
        end
        
        function out = forward(obj, data)
            out = data;
        end
    end
    
end

